          .card.shadow.border-0.text-center
            .card-body
              h5.card-title Card title
              p.card-text This card has a regular title and short paragraphy of text below it.
              p.card-text
                small.text-muted Last updated 3 mins ago
          .card.shadow.border-0
            img.card-img(src='https://4.bp.blogspot.com/-InDD3Hm_bhU/XB4_TK3TT7I/AAAAAAAAAJ4/r5tUeCOqq1MTchFh7D7pWdf582A4qYIIwCEwYBhgL/s1600/Businesswoman-working-at-a-computer-1280x720.jpg', alt='Card image')
